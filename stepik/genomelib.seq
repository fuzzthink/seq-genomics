from extensions import *
from seqlib import Seqs

def pathToGenome(seqs:Seqs) -> seq:
   res = seqs[0]
   n = len(res)-1
   for i, dna in enumerate(seqs[1:]): 
      if res[i+1:] == dna[:-1]:
         res += dna[-1]
      else:
         raise ValueError('pathToGenome() not possible due to seqs not continuous')
   return res
