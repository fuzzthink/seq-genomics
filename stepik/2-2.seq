# 1.2.11 - Find k-Universal Circular String
if run(4):
   kmers = permsOfLen(3, '01')
   s = genomeFromSeqs(kmers, 'universal')
   s |> testVs(s'00101110')

   kmers = permsOfLen(4, '01')
   s = genomeFromSeqs(kmers, 'universal')
   s |> testVs(s'0010011010111100')

   kmers = permsOfLen(9, '01')
   s = genomeFromSeqs(kmers, 'universal')
   s |> testVs(s'')

